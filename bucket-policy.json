{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "AllowCloudFrontOACRead",
      "Effect": "Allow",
      "Principal": {"Service": "cloudfront.amazonaws.com"},
      "Action": ["s3:GetObject"],
      "Resource": ["arn:aws:s3:::mobile-auto-shop-staging-b928aa27/*"],
      "Condition": {
        "StringEquals": {"AWS:SourceArn": "arn:aws:cloudfront::588738589514:distribution/"}
      }
    }
  ]
}
