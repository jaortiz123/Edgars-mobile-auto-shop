{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "predev": "lsof -ti tcp:5173 | xargs kill -9 || true",
    "dev": "vite",
    "dev:clean": "npx kill-port 5173 && vite",
    "kill-port": "npx kill-port 5173",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "lint:ci": "eslint . --ext .ts,.tsx --max-warnings 0",
    "preview": "vite preview",
    "test": "vitest",
    "test:coverage": "vitest run --coverage --reporter=basic",
    "test:coverage-json": "vitest run --coverage --reporter=basic --coverage.reporter=json",
    "test:integration": "vitest run --config vitest.config.ts src/tests/integration",
    "test:strict": "VITEST_STRICT_CONSOLE=true vitest --run",
    "test:ci": "VITEST_STRICT_CONSOLE=true vitest --run --reporter=verbose --silent=false",
    "test:a11y": "vitest run src/tests/accessibility.test.tsx --reporter=verbose",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "clean": "rm -rf node_modules/.cache .vite .next dist"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.1.1",
    "@sentry/react": "^9.31.0",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "@tanstack/react-query": "^5.39.2",
    "axios": "^1.10.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "jwt-decode": "^4.0.0",
    "lucide-react": "^0.522.0",
    "react": "^19.1.0",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.58.1",
    "react-router-dom": "^7.6.2",
    "tailwind-merge": "^3.3.1",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest-axe": "^3.5.9",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@types/react-router-dom": "^5.3.3",
    "@vitejs/plugin-react": "^4.4.1",
    "@vitest/coverage-istanbul": "^3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "autoprefixer": "^10.4.21",
    "axe-core": "^4.10.3",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "eslint-plugin-testing-library": "^7.6.3",
    "globals": "^16.0.0",
    "jest-axe": "^10.0.0",
    "jsdom": "^26.1.0",
    "kill-port": "^2.0.1",
    "msw": "^2.10.4",
    "postcss": "^8.5.6",
    "rollup-plugin-visualizer": "^6.0.3",
    "tailwindcss": "^3.4.16",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.30.1",
    "vite": "^6.3.5",
    "vitest": "^3.2.4",
    "vitest-fail-on-console": "^0.8.0",
    "whatwg-fetch": "^3.6.20"
  }
}
