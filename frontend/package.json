{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "predev": "sh -c 'if [ -z \"$SKIP_KILL_5173\" ]; then command -v lsof > /dev/null && lsof -ti tcp:5173 | xargs kill -9 2>/dev/null || true; else echo \"SKIP_KILL_5173 set; not killing port 5173\"; fi'",
    "dev": "vite",
    "dev:clean": "npx kill-port 5173 && vite",
    "kill-port": "npx kill-port 5173",
    "build": "vite build",
    "lint": "eslint .",
    "lint:ci": "eslint src --ext .ts,.tsx --max-warnings 0 --ignore-pattern 'src/tests/**' --ignore-pattern 'src/test/**' --ignore-pattern 'src/__tests__/**' --ignore-pattern 'src/admin/**' --ignore-pattern 'src/components/admin/**' --ignore-pattern 'src/pages/AdminAppointments.tsx' --ignore-pattern 'src/layout/PublicLayout.tsx' --ignore-pattern 'src/components/admin/AppointmentCardRobust.*'",
    "preview": "vite preview",
    "preview:spa": "npm run build && npx serve -s dist",
    "pretest": "pkill -f \"node .*vitest\" || true",
    "test": "vitest",
    "test:unit": "vitest",
    "test:unit:ci": "vitest run --coverage",
    "test:triage": "vitest --project triage",
    "test:archived": "vitest --project archived",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage --maxConcurrency=4",
    "test:coverage-json": "vitest run --coverage --coverage.reporter=json --maxConcurrency=4",
    "test:coverage-check": "npm run test:coverage && node scripts/coverage/check.js",
    "catalog:validate": "ts-node scripts/validateServiceCatalog.ts",
    "test:integration": "vitest run --config vitest.config.ts src/tests/integration --maxConcurrency=4",
    "test:integration:full": "vitest run --config vitest.integration.config.ts",
    "test:strict": "VITEST_STRICT_CONSOLE=true vitest --run --maxConcurrency=4",
    "test:ci": "VITEST_STRICT_CONSOLE=true vitest --run --reporter=verbose --silent=false --maxConcurrency=4",
    "test:ci-serial": "VITEST_STRICT_CONSOLE=true vitest --run --reporter=verbose --silent=false --maxConcurrency=1",
    "test:a11y": "vitest run src/tests/accessibility.test.tsx --reporter=verbose --maxConcurrency=4",
    "test:e2e": "playwright test",
    "test:e2e:list": "playwright test --list",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "clean": "rm -rf node_modules/.cache .vite .next dist",
    "typecheck": "tsc -p tsconfig.typecheck.json --pretty"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.1.1",
    "@radix-ui/react-slot": "^1.2.3",
    "@sentry/react": "^9.31.0",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "@tanstack/react-query": "^5.39.2",
    "axios": "^1.10.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "lucide-react": "^0.522.0",
    "react": "^19.1.0",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.58.1",
    "react-router-dom": "^7.6.2",
    "recharts": "2.15.4",
    "tailwind-merge": "^3.3.1",
    "zod": "^3.25.67",
    "zustand": "^5.0.7"
  },
  "devDependencies": {
    "@babel/traverse": "^7.25.0",
    "@eslint/js": "^9.25.0",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest-axe": "^3.5.9",
    "@types/node": "^22.17.1",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@types/react-router-dom": "^5.3.3",
    "@types/recharts": "1.8.29",
    "@vitejs/plugin-react": "^4.7.0",
    "@vitest/coverage-istanbul": "^3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "autoprefixer": "^10.4.21",
    "axe-core": "^4.10.3",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "eslint-plugin-testing-library": "^7.6.3",
    "globals": "^16.0.0",
    "jest-axe": "^10.0.0",
    "jsdom": "^26.1.0",
    "kill-port": "^2.0.1",
    "msw": "^2.10.4",
    "postcss": "^8.5.6",
    "rollup-plugin-visualizer": "^6.0.3",
    "tailwindcss": "^3.4.16",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.30.1",
    "vite": "^6.3.5",
    "vitest": "^3.2.4",
    "vitest-fail-on-console": "^0.8.0",
    "whatwg-fetch": "^3.6.20"
  },
  "overrides": {
    "@babel/traverse": "^7.25.0",
    "json5": "^2.2.3",
    "semver": "^7.7.2",
    "trim-newlines": "^4.0.2",
    "braces": "^3.0.3",
    "cross-spawn": "^7.0.6",
    "micromatch": "^4.0.8",
    "got": "^13.0.0",
    "@babel/helpers": "^7.28.3",
    "word-wrap": "^1.2.5",
    "brace-expansion": "^2.0.2"
  }
}
